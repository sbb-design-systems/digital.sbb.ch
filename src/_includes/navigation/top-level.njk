{% set navPages = collections.toplevel | eleventyNavigation(page.lang) %}
<sbb-navigation-marker>
    {%- for entry in navPages %}
        <sbb-navigation-button id="{{ entry.key }}" {{ helpers.getLinkActiveState(entry.url, page.url) | safe }}>{{ entry.title }}</sbb-navigation-button>
    {%- endfor %}
</sbb-navigation-marker>
